{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "google", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "camelcase": [2, { "properties": "always" }],
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
