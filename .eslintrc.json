{
  "env": {
    // Might change "browser"
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "google", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {      "prettier/prettier": [

  "error",

  {

    "endOfLine": "auto"

  }
  ]}
}
